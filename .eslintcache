alse);\n  const [isOpen, setIsOpen] = useState(false);\n\n  const handleDrawer = () => {\n    setIsOpen(!isOpen)\n  }\n\n  const handleBack = () => {\n    setActiveStep((prevActiveStep) => prevActiveStep - 1)\n  }\n\n  /*\n   * This should also be pushing the entered data into the object\n   * This could get complicated. I'll need to give each input a unique ID so that if I backtrack I can have it update\n   * The unique item you'd want to change otherwise stuff could get messy\n   */\n  const handleNext = () => {\n    if (activeStep < steps.length) {\n      setActiveStep((prevActiveStep) => prevActiveStep + 1)\n    } \n  }\n\n  const handleReview = () => {\n    setReadyForReview(true)\n  }\n\n  return (\n    <main>\n      <MainBar />\n      <AppBar \n        position='sticky' \n        elevation={0} \n        className={classes.appBar}\n      >\n        <Toolbar>\n          <IconButton disabled={activeStep === 0} onClick={handleBack}>\n            <ExpandLessIcon />\n          </IconButton>\n          <Next\n            activeStep={activeStep}\n            steps={steps}\n            handleNext={handleNext}\n            handleReview={handleReview}\n           />\n          {\n            /* If you're at the end of your stepper then stop showing the help icon */\n            activeStep <= steps.length - 1\n              ? <IconButton className={classes.infoButton} onClick={handleDrawer}>\n                  <InfoOutlinedIcon />\n                </IconButton>\n              : false\n          }\n        </Toolbar>\n      </AppBar>\n      <div>\n        <InfoDrawer \n          step={activeStep} \n          stepTitle={steps[activeStep]}\n          isOpen={isOpen}\n          handleDrawer={handleDrawer}\n        /> \n        <CocktailStepper \n          steps={steps} \n          activeStep={activeStep} \n          handleNext={handleNext}\n        />\n      </div>\n      <div className={classes.buttonDiv}>\n      {\n        /* Once all steps are completed then display the Review button */\n        activeStep === steps.length\n          ? <Button\n              className={classes.button}\n              variant=\"outlined\"\n              color=\"primary\"\n              href=\"/cocktail\"\n              onClick={() => console.log(\"Went to next\")}\n            >\n              Review\n            </Button>\n          : false\n      }\n      </div>\n    </main>\n  );\n}\n\n","/home/dj/Development/ThirdsMedia/100DaysOfCode/src/CreationWizard/BasicInfo.js",[],"/home/dj/Development/ThirdsMedia/100DaysOfCode/src/CreationWizard/Instructions.js",[],"/home/dj/Development/ThirdsMedia/100DaysOfCode/src/Profile/Profile.js",[],"/home/dj/Development/ThirdsMedia/100DaysOfCode/src/Profile/EditProfile.js",[],"/home/dj/Development/ThirdsMedia/100DaysOfCode/src/Auth/SignIn.js",[],"/home/dj/Development/ThirdsMedia/100DaysOfCode/src/Auth/SignUp.js",[],"/home/dj/Development/ThirdsMedia/100DaysOfCode/src/Products/Products.js",[],"/home/dj/Development/ThirdsMedia/100DaysOfCode/src/Helpers/ItemSheet.js",[],"/home/dj/Development/ThirdsMedia/100DaysOfCode/src/Helpers/MainBar.js",[],"/home/dj/Development/ThirdsMedia/100DaysOfCode/src/Helpers/Copyright.js",[],"/home/dj/Development/ThirdsMedia/100DaysOfCode/src/Products/CardList.js",[],"/home/dj/Development/ThirdsMedia/100DaysOfCode/src/Helpers/QRCode.js",[],"/home/dj/Development/ThirdsMedia/100DaysOfCode/src/CreationWizard/BaseSpirit.js",[],"/home/dj/Development/ThirdsMedia/100DaysOfCode/src/CreationWizard/Ingredients.js",[],"/home/dj/Development/ThirdsMedia/100DaysOfCode/src/Helpers/MetricSelector.js",[],"/home/dj/Development/ThirdsMedia/100DaysOfCode/src/Helpers/ImperialSelector.js",[],"/home/dj/Development/ThirdsMedia/100DaysOfCode/src/static/homeNav.js",[],"/home/dj/Development/ThirdsMedia/100DaysOfCode/src/CreationWizard/CocktailStepper.js",[],"/home/dj/Development/ThirdsMedia/100DaysOfCode/src/CreationWizard/Next.js",[],"/home/dj/Development/ThirdsMedia/100DaysOfCode/src/Navigation/AppDrawer.js",[],"/home/